{
    "global_config": {
        "original_data_file": "../../traces/zeek/raw.csv",
        "overwrite": true,
        "dataset_type": "netflow",
        "n_chunks": 2,
        "dp": false
    },
    "default": "single_event_per_row.json",
    "pre_post_processor": {
        "class": "NetsharePrePostProcessor",
        "config": {
            "timestamp": {
                "column": "ts",
                "generation": true,
                "encoding": "interarrival",
                "normalization": "ZERO_ONE"
            },
            "word2vec": {
                "vec_size": 10,
                "model_name": "word2vec_vecSize",
                "annoy_n_trees": 100,
                "pretrain_model_path": null
            },
            "metadata": [
                {
                    "column": "uid",
                    "type": "string",
                    "encoding": "word2vec_proto"
                },
                {
                    "column": "id.orig_h",
                    "type": "integer",
                    "encoding": "bit",
                    "n_bits": 32,
                    "categorical_mapping": false
                },
                {
                    "column": "id.resp_h",
                    "type": "integer",
                    "encoding": "bit",
                    "n_bits": 32,
                    "categorical_mapping": false
                },
                {
                    "column": "id.orig_p",
                    "type": "integer",
                    "encoding": "word2vec_port"
                },
                {
                    "column": "id.resp_p",
                    "type": "integer",
                    "encoding": "word2vec_port"
                },
                {
                    "column": "proto",
                    "type": "string",
                    "encoding": "word2vec_proto"
                },
                {
                    "column": "service",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "conn_state",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "local_orig",
                    "type": "string",
                    "encoding": "categorical"
                },
                {
                    "column": "missed_bytes",
                    "type": "string",
                    "encoding": "categorical"
                }
            ],
            "timeseries": [
                {
                    "column": "duration",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "orig_bytes",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "resp_bytes",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                                {
                    "column": "orig_pkts",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                },
                {
                    "column": "resp_pkts",
                    "type": "float",
                    "normalization": "ZERO_ONE",
                    "log1p_norm": true
                }
            ]
        }
    },
    "model": {
        "class": "DoppelGANgerTorchModel",
        "config": {
            "batch_size": 100,
            "sample_len": [
                1,
                5,
                10
            ],
            "sample_len_expand": true,
            "epochs": 10,
            "extra_checkpoint_freq": 1,
            "epoch_checkpoint_freq": 5
        }
    }
}